{% extends 'base.html.twig' %}
{% include 'header.html.twig' %}
{% block title %}Hello VehiculeController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

   


    {% for element in vehicule %}
    
    <img src="{{ asset('uploads/' ~ element.images) }}" />
        <h1>{{ element.marque }}</h1>
        <h3>{{ element.modele }}</h3>
        <p><i>Couleur </i>{{ element.couleur }}</p>
        <input type="button" value="reserver">
         <a href="{{ path('DetailVehicule', {'id': vehicule.id}) }}">detail</a> 
    {% endfor %}

{% endblock %}